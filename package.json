{
  "name": "roster-control",
  "version": "1.0.0",
  "description": "Discord bot for managing Squad server rosters with real-time monitoring and admin controls",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "dev:debug": "NODE_ENV=development nodemon --inspect src/index.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy:commands": "node scripts/deploy-commands.js",
    "deploy:commands:dev": "NODE_ENV=development node scripts/deploy-commands.js",
    "deploy:commands:prod": "NODE_ENV=production node scripts/deploy-commands.js",
    "test:db": "node scripts/test-db-connection.js",
    "test:player": "node scripts/test-player-model.js",
    "test:db:full": "node scripts/test-new-database-setup.js",
    "test:models": "node scripts/test-all-new-models.js",
    "test:env": "node scripts/test-environment-detection.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:migrate:dev": "NODE_ENV=development node scripts/migrate-database.js",
    "db:migrate:prod": "NODE_ENV=production node scripts/migrate-database.js",
    "db:migrate:status": "node scripts/migration-status.js",
    "db:migrate:status:dev": "NODE_ENV=development node scripts/migration-status.js",
    "db:migrate:status:prod": "NODE_ENV=production node scripts/migration-status.js",
    "db:migrate:rollback": "node scripts/rollback-migration.js",
    "db:migrate:rollback:dev": "NODE_ENV=development node scripts/rollback-migration.js",
    "db:migrate:rollback:prod": "NODE_ENV=production node scripts/rollback-migration.js"
  },
  "keywords": [
    "discord",
    "bot",
    "squad",
    "roster",
    "whitelist",
    "battlemetrics"
