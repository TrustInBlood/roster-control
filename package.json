  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^1.12.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.2",
    "mariadb": "^3.2.3",
    "node-fetch": "^3.3.2",
    "sequelize": "^6.35.2",
    "socket.io-client": "^4.7.4",
    "umzug": "^3.5.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
