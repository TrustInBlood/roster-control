# Production Environment Configuration
NODE_ENV=production

# Discord Bot Configuration (Production Server)
DISCORD_TOKEN=your_production_discord_bot_token
DISCORD_CLIENT_ID=your_production_client_id
DISCORD_GUILD_ID=your_production_guild_id

# Database Configuration (Production)
DB_HOST=your_production_db_host
DB_PORT=3306
DB_NAME=roster_control
DB_USER=your_production_db_user
DB_PASS=your_production_db_password

# BattleMetrics API (Production Server)
BATTLEMETRICS_TOKEN=your_battlemetrics_token
BATTLEMETRICS_SERVER_ID=your_production_server_id

# SquadJS Configuration (Production)
SQUADJS_HOST=your_squadjs_host
SQUADJS_PORT=3000
SQUADJS_PASSWORD=your_squadjs_password

# Logging Configuration (Production - Less Verbose)
LOG_LEVEL=info
